#include <cstdio>
#include <iostream>
#include <vector>
#include <set>
#include <bits/stdc++.h>
using namespace std;

int main(){
    freopen("paint.in", "r", stdin);
  freopen("paint.out", "w", stdout);
    int a, b; cin >> a >> b;
    int c, d; cin >> c >> d;
    set <int> s;
    for(int i=a; i < b; i++){
        s.insert(i);
    }
    for(int i=c; i < d; i++){
        s.insert(i);
    }
    cout << s.size();
}
